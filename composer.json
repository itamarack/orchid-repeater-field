{
  "name": "diggitto/orchid-repeater-field",
  "description": "Adding support of repeater field to the Orchid RAD platform. Forked from Nks/orchid-repeater-field",
  "license": "wtfpl",
  "version": "v14.0.0",
  "authors": [
    {
      "name": "Mikhail Margasov",
      "email": "m.margasov@diggitto.ru",
      "role": "Developer"
    },
    {
      "name": "Vladislav Zhdanov",
      "email": "v.jdanov@gmail.com",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/itamarack/orchid-repeater-field",
  "keywords": [
    "Laravel",
    "Orchid Repeater Field",
    "Repeater Field"
  ],
  "require": {
    "laravel/framework": "^12",
    "orchid/platform": "^14"
  },
  "require-dev": {
    "mockery/mockery": "^1.1",
    "sempro/phpunit-pretty-print": "^1.0",
    "friendsofphp/php-cs-fixer": "^3.8",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Diggitto\\OrchidRepeaterField\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Diggitto\\OrchidRepeaterField\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Diggitto\\OrchidRepeaterField\\Providers\\ServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
